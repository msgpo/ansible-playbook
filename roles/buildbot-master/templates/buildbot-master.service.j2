# {{ ansible_managed }}

[Unit]
Description=buildbot master
After=network.target

[Service]
User={{ buildbot_master_user }}
Group={{ buildbot_master_group }}
WorkingDirectory={{ buildbot_master_dir }}
ExecStart={{ buildbot_master_sandbox }}/bin/buildbot start {{ buildbot_master_dir }}
LimitNOFILE=16384

[Install]
WantedBy=multi-user.target
